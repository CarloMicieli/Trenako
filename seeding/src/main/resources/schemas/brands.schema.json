{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Brand",
  "type": "object",
  "required": [
    "name",
    "registeredCompanyName",
    "kind",
    "status",
    "version"
  ],
  "properties": {
    "name": {
      "type": "string"
    },
    "registeredCompanyName": {
      "type": "string"
    },
    "organizationEntityType": {
      "anyOf": [
        {
          "$ref": "#/definitions/OrganizationEntityType"
        },
        {
          "type": "null"
        }
      ]
    },
    "kind": {
      "$ref": "#/definitions/Kind"
    },
    "status": {
      "$ref": "#/definitions/Status"
    },
    "groupName": {
      "type": [
        "string",
        "null"
      ]
    },
    "description": {
      "type": [
        "string",
        "null"
      ]
    },
    "contactInfo": {
      "anyOf": [
        {
          "$ref": "#/definitions/Contacts"
        },
        {
          "type": "null"
        }
      ]
    },
    "address": {
      "anyOf": [
        {
          "$ref": "#/definitions/Address"
        },
        {
          "type": "null"
        }
      ]
    },
    "social": {
      "anyOf": [
        {
          "$ref": "#/definitions/Socials"
        },
        {
          "type": "null"
        }
      ]
    },
    "version": {
      "type": "string"
    }
  },
  "definitions": {
    "Address": {
      "type": "object",
      "required": [
        "city",
        "country",
        "postalCode",
        "streetAddress"
      ],
      "properties": {
        "city": {
          "type": "string"
        },
        "country": {
          "type": "string"
        },
        "extendedAddress": {
          "type": [
            "string",
            "null"
          ]
        },
        "postalCode": {
          "type": "string"
        },
        "region": {
          "type": [
            "string",
            "null"
          ]
        },
        "streetAddress": {
          "type": "string"
        }
      }
    },
    "Contacts": {
      "type": "object",
      "properties": {
        "email": {
          "type": [
            "string",
            "null"
          ]
        },
        "phone": {
          "type": [
            "string",
            "null"
          ]
        },
        "websiteUrl": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "Kind": {
      "type": "string",
      "enum": [
        "INDUSTRIAL",
        "BRASS_MODELS"
      ]
    },
    "OrganizationEntityType": {
      "type": "string",
      "enum": [
        "CIVIL_LAW_PARTNERSHIP",
        "ENTREPRENEURIAL_COMPANY",
        "GLOBAL_PARTNERSHIP",
        "LIMITED_COMPANY",
        "LIMITED_PARTNERSHIP",
        "LIMITED_PARTNERSHIP_LIMITED_COMPANY",
        "OTHER",
        "PUBLIC_INSTITUTION",
        "PUBLIC_LIMITED_COMPANY",
        "REGISTERED_SOLE_TRADER",
        "SOLE_TRADER",
        "STATE_OWNED_ENTERPRISE"
      ]
    },
    "Socials": {
      "type": "object",
      "properties": {
        "facebook": {
          "type": [
            "string",
            "null"
          ]
        },
        "instragram": {
          "type": [
            "string",
            "null"
          ]
        },
        "linkedin": {
          "type": [
            "string",
            "null"
          ]
        },
        "twitter": {
          "type": [
            "string",
            "null"
          ]
        },
        "youtube": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "Status": {
      "type": "string",
      "enum": [
        "ACTIVE",
        "OUT_OF_BUSINESS"
      ]
    }
  }
}
