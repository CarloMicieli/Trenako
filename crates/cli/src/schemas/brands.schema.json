{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Brand",
  "type": "object",
  "required": [
    "name",
    "kind",
    "status"
  ],
  "properties": {
    "name": {
      "type": "string"
    },
    "registered_company_name": {
      "type": ["string", "null"]
    },
    "organization_entity_type": {
      "anyOf": [
        {
          "$ref": "#/definitions/OrganizationEntityType"
        },
        {
          "type": "null"
        }
      ]
    },
    "kind": {
      "$ref": "#/definitions/Kind"
    },
    "status": {
      "$ref": "#/definitions/Status"
    },
    "group_name": {
      "type": [
        "string",
        "null"
      ]
    },
    "description": {
      "type": "object",
      "additionalProperties": {
        "type": [ "string", "null" ]
      }
    },
    "contact_info": {
      "anyOf": [
        {
          "$ref": "#/definitions/Contacts"
        },
        {
          "type": "null"
        }
      ]
    },
    "address": {
      "anyOf": [
        {
          "$ref": "#/definitions/Address"
        },
        {
          "type": "null"
        }
      ]
    },
    "social": {
      "anyOf": [
        {
          "$ref": "#/definitions/Socials"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "definitions": {
    "Address": {
      "type": "object",
      "properties": {
        "street_address": {
          "type": [
            "string",
            "null"
          ],
          "minLength": 5,
          "maxLength": 255
        },
        "extendedAddress": {
          "type": [
            "string",
            "null"
          ],
          "maxLength": 255
        },
        "city": {
          "type": [
            "string",
            "null"
          ],
          "minLength": 3,
          "maxLength": 50
        },
        "region": {
          "type": [
            "string",
            "null"
          ],
          "maxLength": 50
        },
        "postal_code": {
          "type": [
            "string",
            "null"
          ],
          "minLength": 3,
          "maxLength": 10
        },
        "country": {
          "type": [
            "string",
            "null"
          ],
          "minLength": 3,
          "maxLength": 3
        }
      }
    },
    "Contacts": {
      "type": "object",
      "properties": {
        "email": {
          "type": [
            "string",
            "null"
          ]
        },
        "phone": {
          "type": [
            "string",
            "null"
          ]
        },
        "website_url": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "Kind": {
      "type": "string",
      "enum": [
        "INDUSTRIAL",
        "BRASS_MODELS"
      ]
    },
    "OrganizationEntityType": {
      "type": "string",
      "enum": [
        "CIVIL_LAW_PARTNERSHIP",
        "ENTREPRENEURIAL_COMPANY",
        "GLOBAL_PARTNERSHIP",
        "LIMITED_COMPANY",
        "LIMITED_PARTNERSHIP",
        "LIMITED_PARTNERSHIP_LIMITED_COMPANY",
        "OTHER",
        "PUBLIC_INSTITUTION",
        "PUBLIC_LIMITED_COMPANY",
        "REGISTERED_SOLE_TRADER",
        "SOLE_TRADER",
        "STATE_OWNED_ENTERPRISE"
      ]
    },
    "Socials": {
      "type": "object",
      "properties": {
        "facebook": {
          "type": [
            "string",
            "null"
          ]
        },
        "instragram": {
          "type": [
            "string",
            "null"
          ]
        },
        "linkedin": {
          "type": [
            "string",
            "null"
          ]
        },
        "twitter": {
          "type": [
            "string",
            "null"
          ]
        },
        "youtube": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "Status": {
      "type": "string",
      "enum": [
        "ACTIVE",
        "OUT_OF_BUSINESS"
      ]
    }
  }
}
